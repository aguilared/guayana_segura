

SELECT 2017 AS 'ano', MONTH(fecha_suceso) AS mes, COUNT(fecha_suceso) As ano1
FROM sucesos
WHERE YEAR(fecha_suceso) = 2017  AND municipio_id = 3 AND delito_detalle_id = 7
GROUP BY MONTH(fecha_suceso)



SELECT
	2017 AS 'ano'
	FROM sucesos
	WHERE YEAR(fecha_suceso) = 2017  AND municipio_id = 3 AND delito_detalle_id = 7


