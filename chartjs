CREATE TABLE `sucesos_mes` (
  `mes` int(10) UNSIGNED NOT NULL,
  `ano1` int(11) DEFAULT '0',
  `ano2` int(11) DEFAULT '0',
  `ano3` int(11) DEFAULT '0',
  
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

